{
    "family": "hpsantiago-task",
    "networkMode": "host",
    "containerDefinitions": [
        {
            "name": "hpsantiago-container",
            "image": "<aws_account_id>.dkr.ecr.<region>.amazonaws.com/home-prices-santiago:latest",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000
                }
            ]            
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],    
    "cpu": "1024",
    "memory": "900"
}